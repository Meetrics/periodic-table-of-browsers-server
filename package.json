{
  "name": "periodic-table-of-browsers-server",
  "version": "1.0.0",
  "description": "Serves browsers and information over the graphql",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "server": "node .dist/server.js",
    "preserver": "npm run compile",
    "start": "nodemon --watch graphQLSchema --exec 'npm run server'",
    "compile": "babel ./graphQLSchema -d .dist/graphQLSchema && babel server.js --out-file .dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meetrics/periodic-table-of-browsers-server.git"
  },
  "keywords": [
    "browsers",
    "properties"
  ],
  "author": "Meetrics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meetrics/periodic-table-of-browsers-server/issues"
  },
  "homepage": "https://github.com/Meetrics/periodic-table-of-browsers-server#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "nodemon": "^1.11.0"
  }
}
